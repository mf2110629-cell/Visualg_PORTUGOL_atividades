Algoritmo "Conte ate 10"
Var
   valor, saltos, contador : inteiro
Inicio
      contador <- 0
      Escreva("Quer contar ate quanto? ")
      leia(valor)
      Escreva("qual o valor do salto? ")
      leia(saltos)
      Enquanto (contador <= valor) faca
         EscrevaL(contador)
         contador <- contador + saltos
fimEnquanto


Fimalgoritmo